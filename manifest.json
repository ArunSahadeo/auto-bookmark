{
    "name": "Auto Bookmark",
    "version": "1.0",
    "manifest_version": 2,
    "author": "Arun James Sahadeo",
    "description": "Automatically bookmarks webpages matching your interests.",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
    }],
    "permissions": [
        "bookmarks",
        "tabs",
        "storage",
        "<all_urls>"
    ]
}
